
<table>
  <tr>
    <th>Post</th>
    
  </tr>
  <% for post in @posts %>
    <tr>
      <td><%=h post.message %></td>
      <td><%= ratings_for post, :static, :small => false, :dimension => :quality %></td>
      <td><%= link_to "Show", post %></td>
      <td><%= link_to "Destroy", post, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New post", new_post_path %></p>
